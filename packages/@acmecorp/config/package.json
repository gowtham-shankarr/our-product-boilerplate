{
  "name": "@acmecorp/config",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./eslint": {
      "types": "./eslint.d.ts",
      "import": "./eslint.js"
    },
    "./prettier": {
      "types": "./prettier.d.ts",
      "import": "./prettier.js"
    },
    "./tailwind": {
      "types": "./tailwind.d.ts",
      "import": "./tailwind.js"
    }
  },
  "files": [
    "index.js",
    "index.d.ts",
    "eslint.js",
    "eslint.d.ts",
    "prettier.js",
    "prettier.d.ts",
    "tailwind.js",
    "tailwind.d.ts",
    "tsconfig.json",
    "tsconfig.base.json",
    "tsconfig.nextjs.json",
    "tsconfig.react-library.json"
