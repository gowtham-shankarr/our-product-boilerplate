{
  "name": "@acmecorp/env",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./server": {
      "types": "./server.d.ts",
      "import": "./server.js"
    },
    "./client": {
      "types": "./client.d.ts",
      "import": "./client.js"
    }
  },
  "files": [
    "index.js",
    "index.d.ts",
    "server.js",
    "server.d.ts",
    "client.js",
    "client.d.ts"
  ],
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  }
}